    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "monolog/monolog": "^1.14"
    },
    "authors": [
        {
            "name": "Justin Martin",
            "email": "frozenfire@thefrozenfire.com"
        },
        {
            "name": "Fernando Marquardt",
            "email": "fernando.marquardt@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "autoload": {
        "classmap": ["nbt.class.php"]
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    }
}
